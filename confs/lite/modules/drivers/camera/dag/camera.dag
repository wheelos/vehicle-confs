# Define all coms in DAG streaming.
module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/camera/libcamera_component.so"

    components {
      class_name : "CameraComponent"
      config {
        name : "camera_front"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_front.pb.txt"
      }
    }

    components {
      class_name : "CameraComponent"
      config {
        name : "camera_back"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_back.pb.txt"
      }
    }

    components {
      class_name : "CameraComponent"
      config {
        name : "camera_left"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_left.pb.txt"
      }
    }

    components {
      class_name : "CameraComponent"
      config {
        name : "camera_right"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_right.pb.txt"
      }
    }
}
