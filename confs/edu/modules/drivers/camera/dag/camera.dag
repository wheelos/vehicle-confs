# Define all coms in DAG streaming.
module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/camera/libcamera_component.so"

    components {
      class_name : "CameraComponent"
      config {
        name : "camera_front"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_front_conf.pb.txt"
      }
    }

    components {
      class_name : "CompressComponent"
      config {
        name : "camera_front_compress"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_front_conf.pb.txt"
        readers {
          channel: "/apollo/sensor/camera/front/image"
          pending_queue_size: 10
        }
      }
    }
}
